# Orpheus Environment Configuration
# Copy this to .env and fill in your keys

# OpenAI (for Codex/GPT executor)
OPENAI_API_KEY=sk-your-openai-key-here

# Anthropic (for Sonnet executor)
ANTHROPIC_API_KEY=sk-ant-your-anthropic-key-here

# Google (for Gemini executor - when ready)
GOOGLE_AI_API_KEY=your-google-ai-key-here

# Orpheus Configuration
ORPHEUS_LOG_LEVEL=info
ORPHEUS_MEMORY_PATH=./orpheus.db
